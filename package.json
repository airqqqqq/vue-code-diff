{
  "name": "vue-code-diff",
  "version": "2.0.0",
  "description": "代码比对展示(Code comparison display)",
  "author": "ddchef<o.o@xjie.me>",
  "private": false,
  "main": "/dist/vue-code-diff.js",
  "keywords": [
    "vue",
    "diff",
    "code"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddchef/vue-code-diff.git"
  },
  "scripts": {
    "start": "node server/server.js",
    "build": "webpack --config ./server/config/webpack.base.js",
    "analyzer": "node server/build.js analyzer",
    "lint": "eslint --ext .js,.vue src --fix"
  },
  "dependencies": {
    "typescript": "^4.2.3",
    "vue": "^3.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@vue/babel-plugin-jsx": "^1.0.3",
    "@vue/compiler-sfc": "^3.0.7",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.9.1",
    "html-webpack-plugin": "^5.3.1",
    "ts-loader": "^8.0.18",
    "vue-loader": "^16.1.2",
    "webpack": "^5.27.0",
    "webpack-cli": "^4.5.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "eslint --ext .js,.vue src"
    }
  },
  "license": "MIT"
}
